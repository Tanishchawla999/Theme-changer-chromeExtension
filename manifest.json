{
    "manifest_version": 3,
    "name": "Theme Randomizer Pro",
    "version": "2.0",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ]
  }
  